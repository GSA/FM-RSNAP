{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "SecureAuth": {
    "RedirectURL": "<insert redirect URL from SecureAuth metadata",
    "TokenName": "<set a token name that makes sense for your app>",
    "ValidationKey": "<insert the validation key that the SecureAuth team sent you>",
    "DecryptionKey": "<insert the decryption key that the SecureAuth team sent you>",
    "UseCompatibilityMode20SP2": true
  },
  "Databases": {
    "RSNAP": {
      "Database": "RSNAPDEV",
      "Username": "RSNAPAPP"
    },
    "Audit": {
      "Database": "FMUTILITYDEV",
      "Username": "FMUTILITY_USER"
    }
  },
  "FMDataAPIKey": "XXXXXX",
  "FMDataAPIURL": "https://localhost:50002",
  "CookieAuthentication": {
    "ExpireMinutes": "6",
    "SessionExpireNotificationMinutes": "5"
  },
  "LogoutRedirectURL": "https://dev-fmitservices.gsa.gov"
}
